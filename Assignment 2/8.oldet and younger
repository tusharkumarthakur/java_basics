//6. most aged and min aged
import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
	int c=sc.nextInt();
	if(a>b){
	    if(a>c)
	      
	      System.out.println("max. age "+a);
	    else
	        System.out.println("max. age "+c);
	}
    else
        if(b>c)
             System.out.println("max.age "+b);
         else
             System.out.println("max. age "+c);

	
		if(a<b){
	    if(a<c)
	      
	      System.out.println("min. age "+a);
	    else
	        System.out.println("min. age "+c);
		}
    else
        if(b<c)
             System.out.println("minage "+b);
         else
             System.out.println("minage "+c);

	
	
	
	
	
	
	
		
				}
}
