// 13.   A 4 digit number is entered through keyboard. Write a program to print a new number with digits reversed as of orignal one. E.g.-
//INPUT : 1234        OUTPUT : 4321
//INPUT : 5982        OUTPUT : 2895

import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt(),rem=0,k,d=0,r=0;
	for(int i=a;i>0;){
		r=i%10;
		rem=r+rem*10;
		i=i/10;
		}
		
		System.out.println(rem);
	}
}
