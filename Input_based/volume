import java.util.Scanner;
class abc{
    float v;
    float volume(float v){
     this.v=v;
     return this.v*this.v*this.v;   
       }    
    }
    class Main{
        
       public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
 System.out.println("enter one side of cube");
        abc ob=new abc();
        ob.v=sc.nextFloat();
   System.out.println(ob.volume(ob.v));
  //volume of cube.      
        } 
        
        }
