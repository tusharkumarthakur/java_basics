//prime number
import java.util.Scanner;
class prime{
    prime(int i){
        if (i%2==0)
        System.out.println("Even number");
        else 
        System.out.println("Odd number");
        
        }    }
        class Main{
       public static void main(String[] args){
     Scanner sc= new Scanner(System.in);
       prime ob= new prime(sc.nextInt());
            
            
            
            }
